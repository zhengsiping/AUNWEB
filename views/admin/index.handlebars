<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">欢迎</h1>
  </div>
</div>
<h3 class="page-header">在线设备</h3>
<table class="table table-striped">
  <thead>
    <tr>
      <th>编号</th>
      <th>设备名</th>
      <th>类型</th>
      <th>所在移动站</th>
      <th>状态</th>
    </tr>
    </thead>
    <tbody>
    {{#onlineDevices}}
      <tr>
        <td>{{id}}</td>
        <td>{{name}}</td>
        <td>{{type.name}}</td>
        <td>{{workstation.name}}</td>
        <td>{{status.status.name}}</td>
      </tr>
    {{/onlineDevices}}
  </tbody>
</table>
<h3 class="page-header">不在线设备</h3>
<table class="table table-striped">
  <thead>
  <tr>
    <th>编号</th>
    <th>设备名</th>
    <th>类型</th>
    <th>所在移动站</th>
    <th>状态</th>
  </tr>
  </thead>
  <tbody>
  {{#offlineDevices}}
    <tr>
      <td>{{id}}</td>
      <td>{{name}}</td>
      <td>{{type.name}}</td>
      <td>{{workstation.name}}</td>
      <td>{{status.status.name}}</td>
    </tr>
  {{/offlineDevices}}
  </tbody>
</table>
